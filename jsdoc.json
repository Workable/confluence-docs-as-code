{
  "sourceType": "module",
  "source": {
    "include": [
      "./lib"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
